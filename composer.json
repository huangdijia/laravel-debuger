{
    "name": "huangdijia/laravel-debuger",
    "description": "debuger for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "debuger"
    ],
    "homepage": "https://github.com/huangdijia/laravel-debuger",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.7|^6.0|^7.0",
        "illuminate/console": "^5.7|^6.0|^7.0",
        "laravel/horizon": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Huangdijia\\Debuger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\Debuger\\DebugerServiceProvider"
            ]
        }
    }
}
